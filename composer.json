{
    "name": "chrisidakwo/laravel-messages",
    "description": "",
    "keywords": ["message", "laravel", "eloquent", "messenger", "messaging", "user", "send", "chat", "system"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Idakwo",
            "email": "chris.idakwo@gmail.com",
            "homepage": "https://github.com/chrisidakwo"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/config": "^5.5|^6.0|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "illuminate/database": "^5.5|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.5",
        "orchestra/testbench": "^3.0|^4.0|^5.0",
        "phpunit/phpunit": "^7.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "ChrisIdakwo\\Messages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisIdakwo\\Messages\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html build/coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisIdakwo\\Messages\\MessagesServiceProvider"
            ]
        }
    }
}
